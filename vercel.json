{
  "buildCommand": "pnpm install && pnpm build",
  "outputDirectory": "client/dist",
  "installCommand": "pnpm install",
  "framework": vite,
  "rewrites": [
    {
      "source": "/api/:path*",
      "destination": "/api/:path*"
    }
  ],
  "routes": [
    {
      "src": "/api/(.*)",
      "dest": "/api/$1"
    },
    {
      "src": "/(.*)",
      "dest": "/client/dist/$1"
    }
  ]
}
